  anime({
    targets: '.vinepart-set:nth-child(1) .vine_one',
    strokeDashoffset: [anime.setDashoffset, 0],
    easing: 'linear',
    duration: 2000,
    delay: 2000,
    direction: 'alternate',
    loop: false,
  });

  anime({
    targets: '.vinepart-set:nth-child(2) .vine_two',
    strokeDashoffset: [anime.setDashoffset, 0],
    easing: 'linear',
